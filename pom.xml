<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>org.ifinal.finalframework.boot</groupId>
    <artifactId>final-boot</artifactId>
    <version>1.1.3-SNAPSHOT</version>
    <name>final-boot</name>
    <description>Demo project for Spring Boot</description>
    <packaging>pom</packaging>
    <modules>
        <module>final-boot-starter</module>
        <module>final-boot-starter-cache</module>
        <module>final-boot-starter-data</module>
        <module>final-boot-starter-dubbo</module>
        <module>final-boot-starter-entity</module>
        <module>final-boot-starter-json</module>
        <module>final-boot-starter-logging</module>
        <module>final-boot-starter-monitor</module>
        <module>final-boot-starter-mybatis</module>
        <module>final-boot-starter-redis</module>
        <module>final-boot-starter-service</module>
        <module>final-boot-starter-sharding</module>
        <module>final-boot-starter-web</module>
    </modules>

    <properties>
        <java.version>1.8</java.version>
        <maven.compiler.source>8</maven.compiler.source>
        <maven.compiler.target>8</maven.compiler.target>
        <final.framework.version>1.1.3-SNAPSHOT</final.framework.version>
        <final.annotation.version>1.1.3-SNAPSHOT</final.annotation.version>
        <final.boot.version>1.1.3-SNAPSHOT</final.boot.version>
        <auto.service.version>1.0.0</auto.service.version>
        <auto.spring.version>1.0.0</auto.spring.version>
    </properties>


    <dependencyManagement>
        <dependencies>
            <!--Spring Boot-->
            <dependency>
                <groupId>org.springframework.boot</groupId>
                <artifactId>spring-boot-starter-parent</artifactId>
                <version>2.3.3.RELEASE</version>
                <scope>import</scope>
                <type>pom</type>
            </dependency>
            <!--Final Annotation-->
            <dependency>
                <groupId>org.ifinal.finalframework.annotation</groupId>
                <artifactId>final-annotation</artifactId>
                <version>${final.annotation.version}</version>
                <scope>import</scope>
                <type>pom</type>
            </dependency>
            <!--Final Framework-->
            <dependency>
                <groupId>org.ifinal.finalframework</groupId>
                <artifactId>final-framework</artifactId>
                <version>${final.framework.version}</version>
                <scope>import</scope>
                <type>pom</type>
            </dependency>
            <!--Final Auto-->
            <dependency>
                <groupId>org.ifinal.finalframework.auto</groupId>
                <artifactId>final-auto</artifactId>
                <version>${final.framework.version}</version>
                <scope>import</scope>
                <type>pom</type>
            </dependency>
            <!--  Final Boot -->
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-cache</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-redis</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-data</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-dubbo</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-entity</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-json</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-logging</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-monitor</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-mybatis</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-service</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-sharding</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-web</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>


</project>

<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.ifinal.finalframework.parent</groupId>
        <artifactId>final-projects-parent</artifactId>
        <version>1.2.0-SNAPSHOT</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>
    <groupId>org.ifinal.finalframework.boot</groupId>
    <artifactId>final-boot</artifactId>
    <version>1.2.0-SNAPSHOT</version>
    <name>final-boot</name>
    <description>Demo project for Spring Boot</description>
    <packaging>pom</packaging>
    <modules>
        <module>final-boot-starter</module>
        <module>final-boot-starter-entity</module>
        <module>final-boot-starter-mybatis</module>
        <module>final-boot-starter-service</module>
        <module>final-boot-starter-redis</module>
        <module>final-boot-starter-cache</module>
        <module>final-boot-starter-sharding</module>
        <module>final-boot-starter-json</module>
        <module>final-boot-starter-logging</module>
        <module>final-boot-starter-monitor</module>
        <module>final-boot-starter-web</module>
    </modules>

    <properties>
        <java.version>1.8</java.version>
        <maven.compiler.source>8</maven.compiler.source>
        <maven.compiler.target>8</maven.compiler.target>
        <final.annotation.version>1.2.0-SNAPSHOT</final.annotation.version>
        <final.framework.version>1.2.0-SNAPSHOT</final.framework.version>
        <final.data.version>1.2.0-SNAPSHOT</final.data.version>
        <final.boot.version>1.2.0-SNAPSHOT</final.boot.version>
        <auto.service.version>1.0.0</auto.service.version>
        <auto.spring.version>1.0.0</auto.spring.version>
    </properties>


    <dependencyManagement>
        <dependencies>
            <!--Final Annotation-->
            <dependency>
                <groupId>org.ifinal.finalframework.annotation</groupId>
                <artifactId>final-annotation</artifactId>
                <version>${final.annotation.version}</version>
                <scope>import</scope>
                <type>pom</type>
            </dependency>
            <!--Final Framework-->
            <dependency>
                <groupId>org.ifinal.finalframework</groupId>
                <artifactId>final-framework</artifactId>
                <version>${final.framework.version}</version>
                <scope>import</scope>
                <type>pom</type>
            </dependency>
            <!--Final Data-->
            <dependency>
                <groupId>org.ifinal.finalframework.data</groupId>
                <artifactId>final-data</artifactId>
                <version>${final.data.version}</version>
                <scope>import</scope>
                <type>pom</type>
            </dependency>
            <dependency>
                <groupId>org.ifinal.auto</groupId>
                <artifactId>auto-service</artifactId>
                <version>${auto.service.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.auto</groupId>
                <artifactId>auto-spring</artifactId>
                <version>${auto.service.version}</version>
            </dependency>
            <!--  Final Boot -->
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-cache</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-redis</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-entity</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-json</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-logging</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-monitor</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-mybatis</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-service</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-sharding</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
            <dependency>
                <groupId>org.ifinal.finalframework.boot</groupId>
                <artifactId>final-boot-starter-web</artifactId>
                <version>${final.boot.version}</version>
            </dependency>
        </dependencies>
    </dependencyManagement>

    <repositories>
        <repository>
            <id>oss</id>
            <name>oss-releases</name>
            <url>https://oss.sonatype.org/content/repositories/releases/</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>oss-snapshots</id>
            <name>oss-snapshots</name>
            <url>https://oss.sonatype.org/content/repositories/snapshots/</url>
            <releases>
                <enabled>false</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
    </repositories>

    <profiles>
        <profile>
            <id>deploy</id>
            <build>
                <plugins>
                    <plugin>
                        <artifactId>maven-javadoc-plugin</artifactId>
                        <executions>
                            <execution>
                                <goals>
                                    <goal>jar</goal>
                                </goals>
                                <phase>package</phase>
                            </execution>
                        </executions>
                        <groupId>org.apache.maven.plugins</groupId>
                    </plugin>
                    <plugin>
                        <artifactId>maven-gpg-plugin</artifactId>
                        <executions>
                            <execution>
                                <goals>
                                    <goal>sign</goal>
                                </goals>
                                <phase>verify</phase>
                            </execution>
                        </executions>
                        <groupId>org.apache.maven.plugins</groupId>
                    </plugin>
                </plugins>
            </build>
            <distributionManagement>
                <repository>
                    <id>ossrh</id>
                    <url>https://oss.sonatype.org/service/local/staging/deploy/maven2/</url>
                </repository>
                <snapshotRepository>
                    <id>ossrh</id>
                    <url>https://oss.sonatype.org/content/repositories/snapshots/</url>
                </snapshotRepository>
            </distributionManagement>
        </profile>
        <profile>
            <id>dmall</id>
            <distributionManagement>
                <repository>
                    <id>dmall.com</id>
                    <url>http://nexus.dmall.com:8081/nexus/content/repositories/3rd</url>
                </repository>
                <snapshotRepository>
                    <id>dmall.com</id>
                    <url>http://nexus.dmall.com:8081/nexus/content/repositories/snapshots</url>
                </snapshotRepository>
            </distributionManagement>
        </profile>
    </profiles>

</project>
